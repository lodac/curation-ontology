[ rdf:type owl:Axiom ;
   owl:annotatedSource :AllowingFileDownload ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Allow access to the data materials by authorized third parties."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#Appraisal
:Appraisal rdf:type owl:Class ;
           rdfs:subClassOf :DataCurationActivities ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass [ rdf:type owl:Class ;
                                           owl:unionOf ( :administrator
                                                         :dataCurator
                                                         :repositorySystem
                                                       )
                                         ]
                           ] ;
           rdfs:comment "Appraisal is the process whereby some records are selected for retention, others (the great majority) are deemed of insufficient value to justify permanent retention."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Appraisal ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Appraisal is the process whereby some records are selected for retention, others (the great majority) are deemed of insufficient value to justify permanent retention."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://www.dcc.ac.uk/guidance/how-guides/appraise-select-data"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#ArrangementAndDescription
:ArrangementAndDescription rdf:type owl:Class ;
                           rdfs:subClassOf :ActualDataProcessing ;
                           rdfs:comment "The re-organization of files (e.g., new folder directory structure) in a dataset that may also involve the creation of new file names, file descriptions, and the recording of technical metadata inherent to the files (e.g., date last modified)."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :ArrangementAndDescription ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "The re-organization of files (e.g., new folder directory structure) in a dataset that may also involve the creation of new file names, file descriptions, and the recording of technical metadata inherent to the files (e.g., date last modified)."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#Authentication
:Authentication rdf:type owl:Class ;
                rdfs:subClassOf :Appraisal ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                                  owl:someValuesFrom :authenticationResults
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                                  owl:someValuesFrom :contactInformation
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.w3.org/ns/prov#used> ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :dataDepositer
                                ] ;
                rdfs:comment "The process of confirming the identity of a person, generally the depositor, who is contributing data to the data repository. (e.g., password authentication or authorization via digital signature). Used for tracking provenance of the data files."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Authentication ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "The process of confirming the identity of a person, generally the depositor, who is contributing data to the data repository. (e.g., password authentication or authorization via digital signature). Used for tracking provenance of the data files."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#CeaseDataCuration
:CeaseDataCuration rdf:type owl:Class ;
                   rdfs:subClassOf :DataPreservationActivities ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                                     owl:someValuesFrom :preservationPolicy
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ;
                                     owl:someValuesFrom :dataCurator
                                   ] ;
                   rdfs:comment "Plan for any contingencies that will ultimately terminate access to the data. For example, providing tombstones or metadata records for data that have been deselected and removed from stewardship."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CeaseDataCuration ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Plan for any contingencies that will ultimately terminate access to the data. For example, providing tombstones or metadata records for data that have been deselected and removed from stewardship."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#ChainOfCustody
:ChainOfCustody rdf:type owl:Class ;
                rdfs:subClassOf :DataProcessing ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :curationLog
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.w3.org/ns/prov#used> ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :submittedData
                                ] ;
                rdfs:comment "Intentional recording of provenance metadata of the files (e.g., metadata about who created the file, when it was last edited, etc.) in order to preserve file authenticity when data are transferred to third-parties."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :ChainOfCustody ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Intentional recording of provenance metadata of the files (e.g., metadata about who created the file, when it was last edited, etc.) in order to preserve file authenticity when data are transferred to third-parties."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#CodeReview
:CodeReview rdf:type owl:Class ;
            rdfs:subClassOf :DataEvaluation ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ;
                              owl:someValuesFrom :dataDepositer
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.w3.org/ns/prov#used> ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :sourceCode
                            ] ;
            rdfs:comment "Run and validate computer code (e.g., look for missing files and/or errors) in order to find mistakes overlooked in the initial development phase, improving the overall quality of software."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :CodeReview ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Run and validate computer code (e.g., look for missing files and/or errors) in order to find mistakes overlooked in the initial development phase, improving the overall quality of software."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#ConnectingDiscoveryServices
:ConnectingDiscoveryServices rdf:type owl:Class ;
                             rdfs:subClassOf :DataPublishing ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty <http://www.w3.org/ns/prov#used> ;
                                               owl:someValuesFrom :fullTextInformation
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ;
                                               owl:someValuesFrom :externalServiceProvider
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty <http://www.w3.org/ns/prov#used> ;
                                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                               owl:onClass :indexingInformation
                                             ] ;
                             rdfs:comment "To connect services that incorporate machine-based search and retrieval functionality that help users identify what data exist, where the data are located, and how can they be accessed (e.g., full-text indexing or web optimization)."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :ConnectingDiscoveryServices ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "To connect services that incorporate machine-based search and retrieval functionality that help users identify what data exist, where the data are located, and how can they be accessed (e.g., full-text indexing or web optimization)."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#Contextualization
:Contextualization rdf:type owl:Class ;
                   rdfs:subClassOf :MetadataProcessing ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                                     owl:someValuesFrom :linkingInformation
                                   ] ;
                   rdfs:comment "Use metadata to link the data set to related publications, dissertations, and/or projects that provide added context to how the data were generated and why."^^rdfs:Literal .

