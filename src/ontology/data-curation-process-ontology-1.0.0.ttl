   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Use metadata to link the data set to related publications, dissertations, and/or projects that provide added context to how the data were generated and why."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#Contracts
:Contracts rdf:type owl:Class ;
           rdfs:subClassOf :externalConstraints ;
           rdfs:comment "An agreement with a research partner, contractor, etc. that restricts the data publishing in joint research or contract research."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Contracts ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "An agreement with a research partner, contractor, etc. that restricts the data publishing in joint research or contract research."^^rdfs:Literal ;
   rdfs:isDefinedBy "http://doi.org/10.5334/dsj-2020-053"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#Conversion
:Conversion rdf:type owl:Class ;
            rdfs:subClassOf :ActualDataProcessing ;
            rdfs:comment "In effort to increase the usability of a data set, the information is transferred into digital file formats (e.g., analog data keyed into a database)."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Conversion ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "In effort to increase the usability of a data set, the information is transferred into digital file formats (e.g., analog data keyed into a database)."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#Copyright
:Copyright rdf:type owl:Class ;
           rdfs:subClassOf :Contracts ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.w3.org/ns/prov#qualifiedUsage> ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :termsOfUse
                           ] .


