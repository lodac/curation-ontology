###  http://www.semanticweb.org/2022/04/data-curation-process#DataCleaning
:DataCleaning rdf:type owl:Class ;
              rdfs:subClassOf :ActualDataProcessing ;
              rdfs:comment "A process used to improve data quality by detecting and correcting (or removing) defects & errors in data."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :DataCleaning ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "A process used to improve data quality by detecting and correcting (or removing) defects & errors in data."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#DataCurationActivities
:DataCurationActivities rdf:type owl:Class ;
                        rdfs:subClassOf :DataLifecycleActivities ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                                          owl:someValuesFrom :curationRecord
                                        ] ;
                        owl:disjointWith :DataPreservationActivities ;
                        rdfs:comment "The encompassing work and actions taken by curators of a data repository in order to provide meaningful and enduring access to data."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :DataCurationActivities ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "The encompassing work and actions taken by curators of a data repository in order to provide meaningful and enduring access to data."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://hdl.handle.net/11299/188638"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#DataEvaluation
:DataEvaluation rdf:type owl:Class ;
                rdfs:subClassOf :DataCurationActivities ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                                  owl:someValuesFrom :evaluationResults
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass [ rdf:type owl:Class ;
                                                owl:unionOf ( :dataCurator
                                                              :peerReviewer
                                                            )
                                              ]
                                ] ;
                rdfs:comment "Evaluation is a decision about significance, value, or quality of something, based on careful study of its good and bad features."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :DataEvaluation ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Evaluation is a decision about significance, value, or quality of something, based on careful study of its good and bad features."^^rdfs:Literal ;
   rdfs:isDefinedBy "https://casrai.org/term/evaluation/"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#DataLifecycleActivities
:DataLifecycleActivities rdf:type owl:Class ;
                         rdfs:subClassOf <http://www.w3.org/ns/prov#Activity> .


###  http://www.semanticweb.org/2022/04/data-curation-process#DataPolicy
:DataPolicy rdf:type owl:Class ;
            rdfs:subClassOf :externalConstraints ;
            rdfs:comment "Where the research funder has a policy on limited data sharing for the research to be funded, or where a strategic business decision restricts the data publishing relating to pending industrial property rights or research data where the commercialization of the research results is envisaged."^^rdfs:Literal .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :DataPolicy ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Where the research funder has a policy on limited data sharing for the research to be funded, or where a strategic business decision restricts the data publishing relating to pending industrial property rights or research data where the commercialization of the research results is envisaged."^^rdfs:Literal ;
   rdfs:isDefinedBy "http://doi.org/10.5334/dsj-2020-053"^^xsd:anyURI
 ] .


###  http://www.semanticweb.org/2022/04/data-curation-process#DataPreservationActivities
:DataPreservationActivities rdf:type owl:Class ;
                            rdfs:subClassOf :DataLifecycleActivities .


###  http://www.semanticweb.org/2022/04/data-curation-process#DataProcessing
:DataProcessing rdf:type owl:Class ;
                rdfs:subClassOf :DataCurationActivities ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass [ rdf:type owl:Class ;
                                                owl:unionOf ( :dataCurator
                                                              :dataDepositer
                                                              :metadataCurator
                                                            )
                                              ]
                                ] ;
                rdfs:comment "A generic concept referring to all kinds of procedures being executed on data at any point in the data life cycle."^^rdfs:Literal .

